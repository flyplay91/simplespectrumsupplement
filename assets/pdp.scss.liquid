	font-size: 3.1vw;
	color: #fff;
	text-align: center;
	line-height: 4.8vw;
}

.header-banner {
	position: relative;
	background-color: #fece33;
}

.header-banner:before,
.header-banner:after {
	content: "";
	position: absolute;
	top: 0;
}

.header-banner:before {
	left: -62vw;
	width: 82.1vw;
	height: 100%;
	background-image: url({{ 'banner-left-bg.png' | asset_url }});
