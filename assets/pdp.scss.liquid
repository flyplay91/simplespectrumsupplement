	}

	.free-from h2 {
		max-width: 64.3vw;
		margin-left: auto;
		margin-right: auto;
		font-size: 3vw;
	}

	.free-from ul li {
		position: relative;
		width: 20%;
		margin-top: 3.7vw;
		padding-left: 4vw;
		font-size: 1.4vw;
		box-sizing: border-box;
		white-space: nowrap;
	}

	.free-from ul li:before {
		top: 0;
		width: 2.3vw;
		height: 2.3vw;
