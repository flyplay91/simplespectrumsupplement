/***** # General *****/

html, body {
  overflow-x: hidden;
}

body {
  font-family: "Poppins";
  font-size: 4.5vw;
  color: #353336;
  line-height: 1.75;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}

.container {
	width: 86.5vw;
	margin: 0 auto;
}

ul {
	list-style-type: none;
	margin: 0;
	padding: 0;
}

a {
	color: inherit;
	text-decoration: none;
}

img {
	width: 100%;
	object-fit: cover;
}

p {
	margin: 0;
}

.bold {
	font-weight: 700;
}

h1 {
	margin-top: 9.4vw;
	margin-bottom: 7.2vw;
	font-size: 7.1vw;
	line-height: 1.2;
}

h2 {
	margin-top: 10.5vw;
	margin-bottom: 3vw;
	font-size: 8.4vw;
	font-weight: 400;
	line-height: 1.4;
}

h3, .bold-ro__recurring-title {
	margin-top: 5vw;
	margin-bottom: 3vw;
	font-size: 5.5vw;
}

.bold-ro__one-time-div {
	display: inline-block;
    height: auto;
    padding: 4vw 8vw;
    margin: 0 auto;
    border: none;
    outline: none;
    font-size: 3.6vw;
    font-weight: 600;
    text-transform: uppercase;
    letter-spacing: .4vw;
    white-space: nowrap;
    cursor: pointer;
    transition: .2s;
    width: 63.8vw;
    margin-bottom: 2.5vw;
    text-align: center;
}

.bold-ro__recurring-div {
	display: inline-block;
    margin: 0 auto;
    height: auto;
    padding: 4vw 4.3vw;
    border: none;
    outline: none;
    font-size: 3.6vw;
    font-weight: 600;
    text-transform: uppercase;
    letter-spacing: .4vw;
    white-space: nowrap;
    cursor: pointer;
    transition: .2s;
    width: 70vw;
    margin-bottom: 2.5vw;
    text-align: center;
}

.bold-ro__recurring-div:not(.red-btn) {
	background-color: #fff;
	border: 2px solid #b1b1b1;
	color: #b1b1b1;
}

.bold-ro__one-time-div:not(.transparent-btn):hover {
	background-color: #ea8777;
}

.bold-ro__one-time-div:not(.transparent-btn) {
	background-color: #e87a68;
	color: #fff;
}

.bold-ro__one-time-radio-btn, .bold-ro__subscribe-radio-btn {
	display: none;
}

.bold-ro__frequency-div {
	display: none;
}

h4 {
	margin-top: 0;
	margin-bottom: 1.4vw;
	font-size: 5.4vw;
	font-weight: 800;
	line-height: 1.4;
}

.btn {
	display: block;
	height: 11.6vw;
	margin: 0 auto;
	padding: 0 8vw;
	border: none;
	outline: none;
	font-size: 3.6vw;
	font-weight: 600;
	text-transform: uppercase;
	letter-spacing: .4vw;
	white-space: nowrap;
	cursor: pointer;
	transition: .2s;
}

.yellow-btn {
	background-color: #fece33;
}

.yellow-btn:hover {
	background-color: #fed75b;
}

.red-btn {
	background-color: #e87a68;
	color: #fff;
}

.red-btn:hover {
	background-color: #ea8777;
}

.blue-btn {
	background-color: #4e72c1;
	color: #fff;
}

.blue-btn:hover {
	background-color: #5f80c7;
}

.transparent-btn {
	background-color: #fff;
	border: 2px solid #b1b1b1;
	color: #b1b1b1;
}

@media (min-width: 1000px) {
	body {
	  font-size: 1.3vw;
	}

	.container {
		width: 81.2vw;
	}

	h1 {
		margin-top: 3.1vw;
		margin-bottom: 1.5vw;
		font-size: 2.2vw;
	}

	h2 {
		margin-top: 2.5vw;
		margin-bottom: .8vw;
		font-size: 3.2vw;
		font-weight: 400;
		line-height: 1.4;
	}

	h3,
