}

.header-banner:after {
	right: -55vw;
	width: 97.1vw;
	height: 100%;
	background-image: url({{ 'banner-right-bg.png' | asset_url }});
	background-size: cover;
}

.header-banner .container {
	display: block;
	margin-top: 2vw;
	padding: 3.6vw 0 4.5vw;
	width: 78vw;
	line-height: 1.4;
	text-align: center;
}

.header-banner .title {
