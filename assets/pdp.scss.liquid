/***** # Product *****/

.product {
	text-align: center;
}

.product img {
	display: block;
	width: 61.6vw;
	margin: 0 auto 7vw;
}

.product .selection {
	position: relative;
	display: flex;
	justify-content: space-between;
	margin-top: 6.4vw;
}

.product .select ul {
	display: none;
	position: absolute;
	bottom: 0;
	left: 0;
	transform: translateY(100%);
}

.product .select ul li,
.product .select .active {
	width: 68.8vw;
	padding-left: 6vw;
	box-sizing: border-box;
	border: 2px solid #e87a68;
	border-top: none;
	font-size: 3.4vw;
	font-weight: 700;
	text-align: left;
	text-transform: uppercase;
	line-height: 1.8;
    padding-top: 4vw;
    padding-bottom: 4vw;
	letter-spacing: .23vw;
	background-color: #fff;
	cursor: pointer;
	padding-right: 25%;
}

.product .select .active {
	position: relative;
	border-top: 2px solid #e87a68;
}

