/***** # Free from *****/

.free-from {
	position: relative;
	padding-top: 9vw;
	padding-bottom: 11vw;
  font-size: 3.7vw;
	color: #fff;
	background-color: #4e72c1;
}

.free-from h2 {
	margin-top: 0;
	margin-bottom: 0;
	font-size: 7.5vw;
	text-align: center;
}

.free-from ul {
	display: flex;
	flex-wrap: wrap;
	justify-content: space-between;
}

.free-from ul li {
	position: relative;
	width: 50%;
	margin-top: 12.5vw;
	padding-left: 14.2vw;
	box-sizing: border-box;
}

.free-from ul li:before {
	content: "\f00c";
	position: absolute;
	top: -1.2vw;
	left: 0;
	width: 8.5vw;
	height: 8.5vw;
	border-radius: 100%;
	border: 2px solid #e87a68;
	background-color: #fff;
	font-family: 'Font Awesome 5 Free';
	font-size: 4.6vw;
	font-weight: 700;
	color: #e87a68;
	line-height: 8.5vw;
	text-align: center;
}

