	.our-people:after {
		content: "";
		position: absolute;
		bottom: -2vw;
		left: 7vw;
		width: 7.4vw;
		height: 16.7vw;
		background-image: url({{ 'flower.png' | asset_url }});
		background-size: cover;
	}

	.our-people h2 {
		margin-top: 2.8vw;
		font-size: 3.7vw;
	}
}

.person {
	width: 100%;
	margin-bottom: 15.4vw;
	padding: 8.4vw 5.3vw 6vw;
	box-sizing: border-box;
	background-color: #fff;
}

.person p {
	display: none;
	margin-bottom: 4vw;
}

.person .text p:first-child {
	display: block;
}

.person h3 {
	margin-top: 9vw;
	margin-bottom: 3vw;
	font-size: 6.3vw;
	font-weight: 800;
	line-height: 1.5;
	hyphens: none;
}

.person img {
	width: 67.6vw;
	max-height: 71.3vw;
	display: block;
	margin: 0 auto;
	object-fit: contain;
}

.person-info {
	padding-left: 2.6vw;
	font-size: 3.3vw;
}

.person .more {
	font-size: 4vw;
	font-weight: 600;
	text-transform: uppercase;
}

.person.red-color {
	border-bottom: 11px solid #ea7a64;
	align-self: flex-end;
}

.person.red-color .more {
	color: #ea7a64;
}

.person.blue-color {
	border-bottom: 11px solid #4c70c4;
	align-self: flex-start;
}

.person.blue-color .more {
	color: #4c70c4;
}

.person.yellow-color {
	border-bottom: 11px solid #ffcf0f;
	align-self: flex-end;
}

.person.yellow-color .more {
	color: #ffcf0f;
}

@media (min-width: 1000px) {
	.persons {
		display: flex;
		flex-direction: column;
	}

	.person {
		display: flex;
		align-items: flex-start;
		width: 67vw;
		margin-bottom: 3.4vw;
		padding: 1.9vw 3.7vw 2.2vw 2.3vw;
		box-sizing: border-box;
		background-color: #fff;
		font-size: .95vw;
	}

	.person p {
		display: none;
		margin-bottom: 1.5vw;
	}

	.person .text p:first-child {
		display: block;
	}

	.person h3 {
		margin-top: 0;
		margin-bottom: 1.6vw;
		font-size: 1.7vw;
		white-space: nowrap;
	}

	.person-info {
		padding-left: 0;
		font-size: .95vw;
	}

	.person img {
		width: 16.7vw;
		height: auto;
		margin-top: .5vw;
		margin-right: 3vw;
	}

	.person .more {
		font-size: 1.2vw;
		font-weight: 600;
		text-transform: uppercase;
	}
}

.popup-bg {
	display: none;
	position: fixed;
	top: 0;
	left: 0;
	width: 100vw;
	height: 100vh;
	background-color: rgba(53,53,53, .5);
}

.popup {
	position: absolute;
	top: 50%;
	left: 50%;
	transform: translate(-50%, -50%);
	width: 90vw;
	max-height: 90vh;
	padding-bottom: 1vw;
	overflow-y: scroll;
	-ms-overflow-style: none;
	overflow: -moz-scrollbars-none;
}

.popup::-webkit-scrollbar { width: 0; }

.popup h3 {
	white-space: normal;
}

.popup.red-color,
.popup.blue-color,
.popup.yellow-color {
	align-self: auto;
}

.popup .close {
	position: absolute;
	top: 1vw;
	right: 2vw;
	font-family: icofont;
	font-size: 6vw;
	cursor: pointer;
}

.popup p {
	display: block;
}

@media (min-width: 1000px) {
	.popup {
		width: 62vw;
		padding-bottom: 1vw;
	}

	.popup .close {
		top: .5vw;
		right: 1.2vw;
		font-size: 2.1vw;
	}
}
