.slick-slider .slick-prev,
.slick-slider .slick-next {
	position: absolute;
	bottom: 0;
	top: auto;
	left: 50%;
	transform: translateX(-1vw);
	width: 28.3vw;
	height: 7.7vw;
	background-image: url({{ 'slider-controls.png' | asset_url }});
	background-size: cover;
}

.slick-slider .slick-prev:before,
.slick-slider .slick-next:before {
	display: none;
}

.slick-slider .slick-prev {
	transform: translateX(-26vw) rotate(180deg);
}

