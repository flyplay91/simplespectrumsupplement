	}

	footer .why-simple {
		max-width: 14.4vw;
		margin-right: 6.1vw;
	}

	footer .useful-links {
		margin-left: 15vw;
	}

	footer ul {
		font-size: .95vw;
	}

	footer ul li {
		margin-bottom: .1vw;
	}

