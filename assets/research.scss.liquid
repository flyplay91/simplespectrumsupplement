}


/***** # Hero *****/

.hero img {
	display: block;
	height: 59.4vw;
}

@media (min-width: 1000px) {
	.hero {
		position: relative;
	}

	.hero .content {
		display: inline-block;
		width: 50vw;
		height: 100%;
		padding: 7vw 0 8.2vw;
		box-sizing: border-box;
		background: linear-gradient(90deg, rgba(255,255,255,1) 0%, rgba(255,255,255,1) 63%, rgba(255,255,255,0) 100%);
	}

	.hero img {
		position: absolute;
		top: 0;
		left: auto;
		right: 0;
		width:61.5vw;
		height: 100%;
		z-index: -1;
