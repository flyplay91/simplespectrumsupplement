/***** # Hero *****/

.hero .slider {
	margin-bottom: 0;
}

.hero .slide {
	position: relative;
	background-color: rgba(255,255,255,.8);
	padding: 8vw 6.7vw 13.8vw;
	text-align: center;
  	height: 105vw;
}

.hero .slide img {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	z-index: -1;
}

.hero .btn {
	margin-top: 6.5vw;
}

.slick-dots {
	position: absolute;
	bottom: 4vw;
	left: 50%;
	transform: translateX(-50%);
}

.slick-dots li button {
	width: 3.6vw;
	height: 3.6vw;
	border: 1px solid #7e7a7a;
	border-radius: 100%;
}

.slick-dots li button:before {
	display: none;
}

.slick-dots .slick-active button {
