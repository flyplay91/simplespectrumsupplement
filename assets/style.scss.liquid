/***** # General *****/

html, body {
  overflow-x: hidden;
}

body {
  font-family: "Poppins";
  font-size: 3.7vw;
  color: #353336;
  line-height: 1.75;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}

.container {
	width: 86.5vw;
	margin: 0 auto;
}

ul {
	list-style-type: none;
	margin: 0;
	padding: 0;
}

a {
	color: inherit;
	text-decoration: none;
}

img {
	width: 100%;
	object-fit: cover;
}

p {
	margin: 0;
}

.bold {
	font-weight: 700;
}

h1 {
	margin-top: 0;
	margin-bottom: 5vw;
	font-size: 9.6vw;
	font-weight: 400;
	line-height: 1.2;
}

h2 {
	margin-top: 0;
	margin-bottom: 4vw;
	font-size: 8.4vw;
	font-weight: 400;
	line-height: 1.4;
}

h4 {
	margin-top: 0;
	margin-bottom: 1.4vw;
	font-size: 5.4vw;
	font-weight: 800;
	line-height: 1.4;
}

.btn {
	display: block;
	height: 11.6vw;
	margin: 0 auto;
	padding: 0 8vw;
	border: none;
	outline: none;
	font-size: 3.6vw;
	font-weight: 600;
	text-transform: uppercase;
	letter-spacing: .4vw;
	white-space: nowrap;
	cursor: pointer;
	transition: .2s;
}

.yellow-btn {
	background-color: #fece33;
}

.yellow-btn:hover {
	background-color: #fed75b;
}

.red-btn {
	background-color: #e87a68;
	color: #fff;
}

.red-btn:hover {
	background-color: #ea8777;
}

.blue-btn {
	background-color: #4e72c1;
	color: #fff;
}

.blue-btn:hover {
	background-color: #5f80c7;
}

@media (min-width: 1000px) {
	body {
	  font-size: 1.3vw;
	}

	.container {
		width: 81.2vw;
	}

	h1 {
		margin-bottom: 2vw;
		font-size: 3.7vw;
		line-height: 1.3;
	}

	h2 {
		margin-top: 0;
		margin-bottom: 1.1vw;
		font-size: 3.2vw;
		font-weight: 400;
		line-height: 1.4;
	}

	h4 {
		margin-top: 0;
		margin-bottom: .7vw;
		font-size: 1.5vw;
		font-weight: 800;
		line-height: 1.4;
	}

	.btn {
		height: 3.3vw;
		margin: 0;
		padding: 0 2.4vw;
		font-size: 1.1vw;
		letter-spacing: .1vw;
	}
}


/***** # Header *****/

.banner {
	padding: 3vw 0 3.1vw;
	background-color: #4e72c1;
}

.banner .container {
	width: 86vw;
	font-size: 2.9vw;
	font-weight: 300;
	color: #fff;
	line-height: 1.2;
	text-transform: uppercase;
	text-align: center;
	letter-spacing: .3vw;
}

header {
	padding-top: 3.4vw;
}

.header-top {
	position: relative;
}

header .container {
	display: flex;
	align-items: center;
}

header .logo {
	width: 46.7vw;
	line-height: 1;
}

.menu-toggle {
	width: 6vw;
	height: 4.6vw;
	margin-left: 19vw;
  position: relative;
  transform: rotate(0deg);
  cursor: pointer;
}

.menu-toggle span {
	display: block;
  position: absolute;
	width: 100%;
	height: .7vw;
	background: #353336;
  opacity: 1;
  left: 0;
  transform: rotate(0deg);
  transition: .3s ease-in-out;
}

.menu-toggle span:nth-child(1) {
  top: 0;
}

.menu-toggle span:nth-child(2),
.menu-toggle span:nth-child(3) {
  top: 2vw;
}

.menu-toggle span:nth-child(4) {
  top: calc(100% - .7vw);
}

.menu-toggle.open span:nth-child(1) {
  top: 2vw;
  width: 0%;
  left: 50%;
}

.menu-toggle.open span:nth-child(2) {
  -webkit-transform: rotate(45deg);
  -moz-transform: rotate(45deg);
  -o-transform: rotate(45deg);
  transform: rotate(45deg);
}

.menu-toggle.open span:nth-child(3) {
  -webkit-transform: rotate(-45deg);
  -moz-transform: rotate(-45deg);
  -o-transform: rotate(-45deg);
  transform: rotate(-45deg);
}

.menu-toggle.open span:nth-child(4) {
  top: calc(100% - .7vw);
  width: 0%;
  left: 50%;
}

.mobile-group {
	display: none;
	position: absolute;
	bottom: 0;
	left: -6.75vw;
	transform: translateY(100%);
	z-index: 10000;
	width: 100vw;
	padding: 6.8vw;
	box-sizing: border-box;
	background-color: #fff;

	font-size: 3.8vw;
	text-transform: uppercase;
}

.header-menu li {
	margin-bottom: 3vw;
}

.header-menu li a {
	position: relative;
	display: inline-block;
}

.header-menu li.active {
	font-weight: 900;
	color: #4c70c4;
}

.header-menu li.active li {
	font-weight: 400;
}

.header-menu li ul {
	display: none;
	margin: 3vw 0;
}

.header-menu .open-submenu {
	position: absolute;
	top: 50%;
	right: -3vw;
	transform: translate(100%, -50%);
	width: 4vw;
	height: 100%;
  font-size: 3.5vw;
  font-weight: 700;
}

header .user,
header .cart .icon {
	font-family: 'simple-line-icons';
	font-size: 5.8vw;
}

header .user a,
header .cart .icon a {
	display: block;
}

header .user {
	position: absolute;
	top: 6vw;
	right: 6.8vw;
}

header .cart {
	position: relative;
	margin-left: 6.5vw;
}

header .cart .product-number {
	position: absolute;
	top: 0;
	right: -2.7vw;
	display: block;
	width: 4.8vw;
	height: 4.8vw;
	border-radius: 100%;
	background-color: #e87a68;
	font-size: 3.1vw;
	color: #fff;
	text-align: center;
	line-height: 4.8vw;
}

.header-banner {
	position: relative;
	background-color: #fece33;
}

.header-banner:before,
.header-banner:after {
	content: "";
	position: absolute;
	top: 0;
}

.header-banner:before {
	left: -62vw;
	width: 82.1vw;
	height: 100%;
	background-image: url({{ 'banner-left-bg.png' | asset_url }});
	background-size: cover;
}

.header-banner:after {
	right: -55vw;
	width: 97.1vw;
	height: 100%;
	background-image: url({{ 'banner-right-bg.png' | asset_url }});
	background-size: cover;
}

.header-banner .container {
	display: block;
	margin-top: 2vw;
	padding: 3.6vw 0 4.5vw;
	width: 78vw;
	line-height: 1.4;
	text-align: center;
}

.header-banner .title {
	font-family: "Daydreamer";
	font-size: 9.9vw;
	line-height: 1;
}

.header-banner .text {
	margin-top: .2vw;
	font-size: 3.3vw;
	font-weight: 700;
}

